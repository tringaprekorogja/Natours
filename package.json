{
  "name": "starter",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "dependencies": {
    "autoprefixer": "^10.0.4",
    "concat": "^1.0.3",
    "postcss": "^8.1.0"
  },
  "devDependencies": {
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.0"
  },
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start":"npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css css/style.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css":"npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tringaprekorogja/Natours.git"
  },
  "author": "Tringa", 
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tringaprekorogja/Natours/issues"
  },
  "homepage": "https://github.com/tringaprekorogja/Natours#readme"
}
